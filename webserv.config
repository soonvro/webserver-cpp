#asd
http {
  server {
    listen  80; 
    location / {
        limit_except GET DELETE {
          deny all;
        }
        root html/;
        autoindex on;
        error_page 404 404.html;
    }
    location /upload_files {
        limit_except GET {
          deny all;
        }
        root upload_files;
        autoindex on;
    }
    location /html {
        limit_except GET {
          deny all;
        }
        client_max_body_size 1;
        root html/;
        return 301 /index.html;
    }
    location ~ .pl {
        limit_except POST GET {
          deny all;
        }
        root html;
        autoindex on;
        cgi_root /usr/bin/perl;
    }
    location ~ .py {
        limit_except POST GET {
          deny all;
        }
        root html;
        autoindex on;
        cgi_root /usr/bin/python3;
    }
  }
    
  server {
    listen  81;
    server_name localhost;
    location / {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane;
        autoindex on;
    }
    location /directory {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane;
        autoindex on;
    }
    location /directory/nop {
        limit_except GET {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane/nop;
        autoindex on;
    }
    location /post_body {
        limit_except POST {
          deny all;
        }
        index  youpi.bad_extension;
        root YoupiBanane/nop;
        autoindex on;
        client_max_body_size 100;
    }
    location ~ .bla {
      root ./;
      cgi_root ./cgi_tester;
    }
  }
}
